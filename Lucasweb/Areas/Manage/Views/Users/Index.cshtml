@model Lucasweb.Areas.Manage.Models.UsersModel

@{
    ViewBag.Title = "index";
}

<h2>index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Users.FirstOrDefault().Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Users.FirstOrDefault().FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Users.FirstOrDefault().LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Users.FirstOrDefault().UserName)
        </th>
    </tr>

@foreach (var item in Model.Users) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
    </tr>
}
    @Html.ActionLink("< Last","Index",new { F_index = Model.Last.F_index, L_index = Model.Last.L_index}) |
    @Html.ActionLink("Next >","Index",new { F_index = Model.Next.F_index, L_index = Model.Next.L_index})
</table>
